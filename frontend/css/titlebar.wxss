.titlebar {
    width: 100%;
    height: 120rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background-color: lightgray;
}
.titlebar-name {
    padding-left: 24rpx;
    padding-right: 24rpx;
    color: white;
}
.titlebar-image {
    width: 80rpx;
    height: 80rpx;
}
.titlebar-left {
    transform:  rotateY(180deg);
}